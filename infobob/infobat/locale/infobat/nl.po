#: infobat/irc.py:513
#, python-format
msgid ""
"%(chance)0.6f%% chance (1 in %(inverse)s) across %(prob_len)d probabilities; "
"%(avg)0.6f%% average, standard deviation %(deviation)0.6f%%, %(min_prob)0.6f%"
"% low, %(max_prob)0.6f%% high."
msgstr ""
"%(chance)0.6f%% kans (1 per %(inverse)s) over %(prob_len)d mogelijkheden; "
"gemiddeld %(avg)0.6f%%, standaard afwijking %(deviation)0.6f%%, ondergrens %"
"(min_prob)0.6f%%, bovengrens %(max_prob)0.6f%%."

